Xft.dpi: 96
Xft.antialias: true
Xft.hinting: true
Xft.rgba: rgb
Xft.autohint: false
Xft.hintstyle: hintslight
Xft.lcdfilter: lcddefault

! URxvt config

URxvt*termName:           rxvt-256color
URxvt.scrollBar:          off
URxvt.font:               xft:LiberationMono:size=12:antialias=true
URxvt.boldFont:           xft:LiberationMono:size=12:antialias=true
URxvt.internalBorder:     1
URxvt*selectToClipboard:  true
URxvt*saveLines:          100000

URxvt.perl-ext-common:    default,clipboard,url-select,keyboard-select,matcher,searchable-scrollback
URxvt.url-select.launcher:        /usr/bin/chromium
URxvt.url-select.underline: true
URxvt.matcher.button:     1

URxvt.keysym.M-u: perl:url-select:select_next
URxvt.keysym.M-Escape: perl:keyboard-select:activate
URxvt.keysym.M-s: perl:keyboard-select:search

! Not so wide please
URxvt.letterSpace: -1

URxvt*xftAntialias: true

! Zenburn like color-scheme
URxvt.background        :#3f3f3f
URxvt.foreground        :#dcdccc
URxvt*cursorColor       :#aaaaaa
URxvt*colorUL           :#93e0e3
URxvt*underlineColor    :#366060
! black + red
URxvt.color0            :#3f3f3f
URxvt.color1            :#cc9393
! green + yellow
URxvt*color2            :#7f9f7f
URxvt*color3            :#d0bf8f
! blue + purple
URxvt*color4            :#6ca0a3
URxvt*color5            :#dc8cc3
! cyan + white
URxvt*color6            :#93e0e3
URxvt*color7            :#dcdccc
! bright-black + bright-red
URxvt*color8            :#000000
URxvt*color9            :#dca3a3
! bright-green + bright-yellow
URxvt*color10           :#bfebbf
URxvt*color11           :#f0dfaf
! bright-blue + bright-purple
URxvt*color12           :#8cd0d3
URxvt*color13           :#dc8cc3
! bright-cyan + bright-white
URxvt*color14           :#93e0e3
URxvt*color15           :#ffffff
